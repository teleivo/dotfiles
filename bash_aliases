alias vu='vagrant up'

alias dps='docker ps --format "table {{.ID}}\t{{.Names}}\t{{.Image}}\t{{.Ports}}"'
alias dGetIp='docker inspect -f "{{.NetworkSettings.IPAddress}}"'
alias dGetNetwork='docker inspect  -f "{{json .HostConfig.NetworkMode}}"'
alias dpsq='docker ps -q'
alias dst='docker start'
alias dsp='docker stop'
alias drm='docker rm'
alias drmRunning='docker rm -f $(docker ps -q)'
alias drmi='docker rmi'
alias dv='docker volume'
alias dvl='docker volume ls'
alias dvr='docker volume rm'
alias dvrA="docker volume ls | awk '{ if ( NR>1 ) { print $2 }; }' | xargs docker volume rm"
alias dexi='docker exec -it'
alias dl='docker logs -tf'
alias dc='docker-compose'

alias goi='go install'
alias gob='go build'
alias gor='go run'

alias gpgKill='gpgconf --verbose --kill gpg-agent'
alias gpgReload='gpgconf --verbose --reload gpg-agent'
