alias ga='git add'
alias gA='git add -A'
alias gb='git branch'
alias gbd='git branch -d'
alias gbD='git branch -D'
alias gc='git commit'
alias gcm='git commit -m'
alias gca='git commit --amend'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gcod='git checkout -- :/'
alias gd='git diff'
alias gds='git diff --staged'
alias gf='git fetch'
alias gfb='git fetch bgevam'
alias gfi='git fetch internal'
alias gfo='git fetch origin'
alias gfu='git fetch upstream'
alias gl='git log --oneline --decorate --all --graph'
alias gm='git merge'
alias gp='git push'
alias gpf='git push --force'
alias gpl='git pull'
alias gplrbum='git pull --rebase upstream master'
alias grb='git rebase'
alias grbm='git rebase master'
alias gre='git remote -vv'
alias grepb='git remote prune bgevam'
alias grepi='git remote prune internal'
alias grepo='git remote prune origin'
alias grepu='git remote prune upstream'
alias grs='git reset'
alias grssh='git reset --soft HEAD^'
alias grsHh='git reset --hard HEAD^'
alias gs='git status'
alias gso='git show'
alias gsos='git show --stat'

alias mcp='mvn clean package'
alias mcps='mvn clean package -Dmaven.test.skip=true'

alias vs='vagrant status'
alias vu='vagrant up'
alias vssh='vagrant ssh'
alias vd='vagrant destroy -f'
