alias ga='git add'
alias gA='git add -A'
alias gb='git branch -vv'
alias gbd='git branch -d'
alias gbDelete='git branch -D'
alias gbv='git branch -vv'
alias gc='git commit'
alias gcm='git commit -m'
alias gca='git commit --amend'
alias gcaNoedit='git commit --amend --no-edit'
alias gwip='git commit -m "WIP" --allow-empty'
alias gco='git checkout'
alias gcoLast='git checkout -'
alias gcob='git checkout -b'
alias gcod='git checkout -- :/'
alias gcom='git checkout master'
alias gd='git diff'
alias gds='git diff --staged'
alias gf='git fetch'
alias gfo='git fetch origin'
alias gfu='git fetch upstream'
alias gl='git log --oneline --decorate --all --graph'
alias gm='git merge'
alias gp='git push'
alias gpForce='git push --force'
alias gpl='git pull --rebase'
alias gplrbum='git pull --rebase upstream master'
alias grb='git rebase'
alias grbi='git rebase -i'
alias grbm='git rebase master'
alias gre='git remote -vv'
alias grepo='git remote prune origin'
alias grepu='git remote prune upstream'
alias grs='git reset'
alias grssh='git reset --soft HEAD^'
alias grsH='git reset --hard'
alias grsHh='git reset --hard HEAD^'
alias gs='git status --short'
alias gso='git show --show-signature'
alias gsos='git show --stat'

alias mcp='mvn clean package'
alias mcps='mvn clean package -DskipTests'
alias mcis='mvn clean install -DskipTests'

