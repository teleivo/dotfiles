#!/bin/sh

(
    cd ~/code/dhis2/dhis2-core/ || exit
    mvn clean install -Pdev -Pjdk11 -f dhis-2/pom.xml -pl -dhis-web-embedded-jetty -Dmaven.test.skip=true && \
